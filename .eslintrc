{
  "extends": "airbnb",

  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },

  "rules": {
    "no-mixed-operators": [
      "error",
      {
        "allowSamePrecedence": true
      }
    ],
    "no-plusplus": "off",
    "max-len": [
      "error",
      {
        "code": 100
      }
    ]
  }
}
